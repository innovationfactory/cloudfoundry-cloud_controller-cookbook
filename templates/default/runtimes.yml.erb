---
<%- node[:cloudfoundry_dea][:runtimes].each do |_, runtime| %>
<%= runtime[:name] %>:
  version: <%= runtime[:version] %>
  version_flag: <%= runtime[:version_flag] %>
  executable: <%= runtime[:executable] %>
<%- if runtime[:debug_env] %>
  debug_modes:
<%- runtime[:debug_env].each do |state, _| %>
     - <%= state %>
<%- end # debug_env.each %>
<%- end # if debug_env %>
<%- end # runtimes.each %>
